---
interface dataShape {
  userId: number;
  id: number;
  title: string;
  body: string;
}
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
const data: dataShape[] = await response.json();
---

<div>
  <h1>Get</h1>
  <div class="flex flex-wrap gap-2 justify-center">
    {
      data.map((el) => (
        <div class="border-2 border-black w-40 p-2">
          <h1>{`UserId: ${el.userId} - ID: ${el.id}`}</h1>
          <h1>{el.title}</h1>
          <p>{el.body}</p>
        </div>
      ))
    }
  </div>
</div>
